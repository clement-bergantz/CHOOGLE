<%= simple_form_for([@choogle]) do |f| %>
  <%= f.error_notification %>
  <%= f.simple_fields_for :proposal do |g| %>

    <%= g.input :place, label: "Select a place" %>
    <%= g.simple_fields_for :proposal_tags do |h| %>
      <%= h.input :tags, label: "Add tags to this place" %>
    <% end %>
  <% end %>

  <%= f.button :submit, class: 'btn btn-primary' %>
<% end %>
